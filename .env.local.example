NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Admin Authentication
# Separate passwords for each role (recommended for production)
ADMIN_PASSWORD="your-admin-password"
EDITOR_PASSWORD="your-editor-password"
VIEWER_PASSWORD="your-viewer-password"

# Admin Session Secret (used for JWT signing)
ADMIN_SESSION_SECRET="your-session-secret-at-least-32-chars"

# Upload Security
# Secret token for upload route authentication
UPLOAD_ROUTE_SECRET="your-upload-route-secret"
NEXT_PUBLIC_UPLOAD_ROUTE_SECRET="your-public-upload-secret"

# Development Settings
# Set to 'true' to enable fallback mock data in development (MUST be 'false' in production)
ENABLE_FALLBACK="false"

# Resend Email Configuration
RESEND_API_KEY="re_your_resend_api_key"
EMAIL_FROM="AutoExport <noreply@yourdomain.com>"
ADMIN_EMAIL="admin@yourdomain.com"
